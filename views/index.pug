extends layout

block content
  h1= title
  p Hello #{title}
  button(type='button' class='btn btn-primary',onclick="update()") Press me for contents
  div(id='target', onclick="update()") Display
  script.
    var client_count=0;
    function update(){
        client_count = client_count+1;
        document.getElementById("target").innerHTML=`Value now ${client_count}`
    }
  
    